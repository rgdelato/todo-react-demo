{
  "name": "todo-react-demo",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "rm -rf build && NODE_ENV=\"development\" webpack-dashboard -- webpack-dev-server --open --inline --env development",
    "build:dev": "rm -rf build && NODE_ENV=\"development\" webpack --env development",
    "build": "rm -rf build && NODE_ENV=\"production\" webpack -p --env production",
    "build:analyze": "npm run build && webpack-bundle-analyzer build/stats.json build",
    "lint": "eslint src",
    "precommit": "npm run lint && staged_files=$(git diff --name-only --cached) && staged_js_files=$(node precommit-filter.js ${staged_files}) && if [ \"$staged_js_files\" == \"\" ]; then exit 0; fi && prettier --write ${staged_js_files} && git add ${staged_js_files}"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgdelato/todo-react-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rgdelato/todo-react-demo/issues"
  },
  "homepage": "https://github.com/rgdelato/todo-react-demo#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-react-app": "^2.0.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "eslint": "^3.8.1",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "html-webpack-plugin": "^2.26.0",
    "husky": "^0.12.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "preact": "^7.1.0",
    "preact-compat": "^3.11.0",
    "prettier": "^0.13.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0-beta.5",
    "style-loader": "^0.13.1",
    "styled-classnames": "^1.1.1",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^1.16.2"
  }
}
